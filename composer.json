{
    "name": "croix-rouge-france/applovin",
    "description": "Applovin, une plateforme d'investissement en cryptomonnaies et paiements Mobile Money",
    "type": "project",
    "require": {
        "php": "^8.2",
        "paydunya/paydunya": "^1.0",
        "endroid/qr-code": "^5.0",
        "web3p/web3.php": "^0.3.2",
        "vlucas/phpdotenv": "^5.5",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-openssl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CroixRougeFrance\\Applovin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "mr Hokague",
            "email": "contact@example.com"
        }
    ],
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "post-install-cmd": [
            "chmod -R 755 vendor"
        ],
        "post-update-cmd": [
            "chmod -R 755 vendor"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
