{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "*.php",
      "use": "@vercel/php"
    },
    {
      "src": "includes/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "*.{html,css,js}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  
}
